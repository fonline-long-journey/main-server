void sinf( Critter& player, int param0, int param1, int param2 )
{
	if( player.ParamBase[ ST_MODER ] == 0 )
	{
		player.ParamBase[ ST_MODER ] = 1;
		player.Say( SAY_NETMSG, "|0xFFFF00 Режим паука активирован." );
	}
	else
	{
