void unsafe_GM_PANNEL_SELF( Critter& player, int param0, int param1, int param2, string@ param3, int[] @ param4 )
{
	if( player.GetAccess() >= ACCESS_MODER ) // Меню - Основное меню.
	{
		string[] List = { "Склад", "Действие по ID", "Паучье зрение" };
